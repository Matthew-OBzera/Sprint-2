<p id="notice"><%= notice %></p>

<p>
  <strong>Start time:</strong>
  <%= @session.start_time %>
</p>

<p>
  <strong>End time:</strong>
  <%= @session.end_time %>
</p>

<p>
  <strong>Session teacher:</strong>
  <%= @session.session_teacher %>
</p>

<p>
  <strong>Session student:</strong>
  <%= @session.session_student %>
</p>

<div class="row">
    <aside class="col-md-4">
      <section class="note">
        <%= form_for(@session_note) do |f| %>
          <div class="field">
            <%= f.text_area :note, placeholder: "Compose new note..." %>
          </div>
          <%= f.submit "Create", class: "btn btn-primary" %>
        <% end %>
      </section>
    </aside>
</div>

<%= link_to 'Edit', edit_session_path(@session) %> |
<%= link_to 'Back', sessions_path %>
